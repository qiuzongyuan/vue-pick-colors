{
  "name": "vue-pick-colors",
  "version": "1.3.1",
  "description": "A Color picker for Vue.js 3",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && npm run lint && rollup -c ./rollup.config.ts",
    "build:dev": "npm run clean && npm run lint && rollup -wc ./rollup.config.ts",
    "dev": "vite ./demo",
    "prepublishOnly": "npm run build",
    "lint": "eslint ./src --ext .vue,.js,.ts,.jsx,.tsx",
    "lint:fix": "eslint --fix ./src --ext .vue,.js,.ts,.jsx,.tsx",
    "clean": "rimraf ./dist",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "color",
    "picker",
    "pick",
    "component"
  ],
  "author": "ayuan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/qiuzongyuan/vue-pick-colors"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@sucrase/jest-plugin": "^2.2.0",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/parser": "^4.31.2",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/test-utils": "2.0.0-rc.17",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^26.6.3",
    "canvas": "^2.11.2",
    "eslint": "7.32.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-vue": "^7.20.0",
    "jest": "^26.6.3",
    "less": "^4.1.2",
    "postcss": "^8.4.5",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.63.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-vue": "^6.0.0",
    "ts-jest": "^26.5.6",
    "typescript": "4.4.4",
    "vite": "^2.7.12",
    "vite-plugin-eslint": "^1.3.0",
    "vue": "^3.2.26",
    "vue-jest": "^5.0.0-alpha.10"
  },
  "peerDependencies": {
    "vue": "^3.2.26"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.2"
  }
}
